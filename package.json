{
  "name": "@twy-gmbh/erc725-playground",
  "version": "0.1.24",
  "description": "This is only used for experimentation with the Solidity-Ecosystem. Nothing to see here.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "release": "npm run clean && npm run build && standard-version",
    "build": "npm run compile && npm run typechain && tsc && cp -r typechain/*.d.ts dist/",
    "clean": "rm -rf dist",
    "lint": "solhint 'contracts/**/*.sol'",
    "compile": "hardhat compile",
    "watcher": "npm run build",
    "test": "jest --runInBand",
    "tdd": "jest --watch"
  },
  "author": "Reto Ryter",
  "license": "MIT",
  "devDependencies": {
    "@ethereum-waffle/jest": "^3.1.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^2.0.0",
    "@types/node": "^14.14.2",
    "chokidar": "^3.4.3",
    "ethereum-waffle": "^3.1.2",
    "ethers": "^5.0.19",
    "hardhat": "^2.0.2",
    "hardhat-typechain": "^0.3.3",
    "jest": "^26.6.1",
    "jest-environment-node-single-context": "^26.2.0",
    "nodemon": "^2.0.6",
    "standard-version": "^9.0.0",
    "ts-generator": "^0.1.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typechain": "^3.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "solc": "0.6.7",
    "solidity-bytes-utils": "^0.1.1"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "contracts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rryter/solidity-playground.git"
  },
  "keywords": [
    "Solidity"
  ],
  "bugs": {
    "url": "https://github.com/rryter/solidity-playground/issues"
  },
  "homepage": "https://github.com/rryter/solidity-playground#readme"
}
